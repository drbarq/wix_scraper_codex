{
  "name": "wix-archiver",
  "version": "0.1.0",
  "description": "Static archive generator for qr646.com (Wix)",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "archive": "npm run discover && npm run capture && npm run assets && npm run process",
    "discover": "node src/crawler.js",
    "capture": "node src/scraper.js",
    "assets": "node src/assetDownloader.js",
    "process": "node src/rewriter.js",
    "preview": "node src/server.js",
    "deploy": "node src/deploy.js"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "cheerio": "^1.0.0-rc.12",
    "commander": "^11.0.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "p-limit": "^3.1.0",
    "playwright": "^1.40.0"
  }
}

